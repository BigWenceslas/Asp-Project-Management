@model IEnumerable<AITECRM.Models.EmailServeur>

<html>
<head>

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Aite Crm Serveur Email</title>
</head>
<body>
    <h4>
        <div class="container-fluid">
            <div class="row">
                <nav class="col-sm-3 col-md-2 d-none d-sm-block bg-light sidebar">
                    @Html.Partial("Menu")
                </nav>
                <main class="col-sm-9 ml-sm-auto col-md-10 pt-3" role="main" id="Donnees">
                    <br /><br /><br /><br />
                    <h4>
                        LISTE DES CONFIGURATIONS &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;<div class="btn-group" role="group" aria-label="Basic example">
                        </div>
                        @Html.ActionLink("NOUVEAU", "Create", "EmailServeurs", new { area = "" }, new { @class = "btn btn-outline-success" })
                        &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;<div class="btn-group" role="group" aria-label="Basic example">
                        </div>

                    </h4>

                    <div class="table-responsive">
                    </div>


                    <h4>
                        <table class="table">
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.Port)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.DateAjout)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ServeurAdresse)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.AdresseCRM)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.PassWordCRM)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.EtatConfig)
                                </th>
                                <th></th>
                            </tr>

                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Port)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.DateAjout)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ServeurAdresse)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.AdresseCRM)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PassWordCRM)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.EtatConfig)
                                    </td>
                                    <td>
                                        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                                        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                                        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                                    </td>
                                </tr>
                            }

                        </table>
                    </h4>
                    @Html.Partial("PagePrecedente")
                    <footer>
                        <center><> <strong>Copyright © 2016 Tout droits réservés. Par <a href="http://www.aite-eonsulting.com">Aite Consulting</a>.</strong></center>
                    </footer>
                </main>

            </div>

        </div>

    </h4>
</body>

</html>




